$sale_shop_id = SaleShop.search([[&apos;name&apos;, &apos;ilike&apos;, &apos;kyle&apos;]])[0] || 1

def do_export
	puts &apos;SKU&apos; + &apos;       &apos; + &apos;Name&apos;
    pos_categ_id = $row[&apos;id&apos;]
  #  puts pos_categ_id
    pos_tax_id = $row[&apos;tax_id&apos;]
  #  puts pos_tax_id
    ref_id = ExternalReferential.search([&apos;name&apos;, &apos;ilike&apos;, &apos;openbravopos&apos;])[0]
    model_id = IrModel.search([&apos;model&apos;, &apos;=&apos;, &apos;product.product&apos;])[0]
    map = ExternalMapping.find(:first, :domain =&gt; [[&apos;referential_id&apos;, &apos;=&apos;, ref_id], [&apos;model_id&apos;, &apos;=&apos;, model_id]])
    last_export_date = map.last_export_date
    if last_export_date
        last_export_date = last_export_date.strftime(&quot;%Y-%m-%d %H:%M:%S&quot;)
    end
    #puts &quot;last_export_date&quot;, last_export_date
    product_ids = ProductProduct.get_modified_ids(last_export_date)
    list = []
    verif = []
    errors = []
    if product_ids and product_ids != []
        product_ids.each do |pr|
            product_attr = ProductProduct.read(pr[0], [&apos;categ_id&apos;, &apos;name&apos;, &apos;default_code&apos;, &apos;price&apos;, &apos;barcode&apos;, &apos;standard_price&apos;, &apos;list_price&apos;, &apos;magento_sku&apos;, &apos;x_magerp_special_price&apos;, &apos;type&apos;, &apos;product_type&apos;, &apos;sale_ok&apos;])
            [&apos;categ_id&apos;].each do |key|
                product_attr[key] = product_attr[key][1] # gives the name
            end
		#	puts product_attr[&apos;default_code&apos;] + &apos;        &apos; + product_attr[&apos;name&apos;] + &apos;    &apos; + product_attr[&apos;price&apos;]
            product_attr[&apos;oe_id&apos;] = &quot;OE_PRODUCT_&quot; + pr[0].to_s
            product_attr[&apos;modified_date&apos;] = pr[1]
            product_attr[&apos;pos_categ_id&apos;] = pos_categ_id # solves strange bug...
            product_attr[&apos;pos_tax_id&apos;] = pos_tax_id     # solves strange bug...
            product_attr[&apos;name1&apos;] = product_attr[&apos;name&apos;]
            product_attr[&apos;name&apos;] = product_attr[&apos;name&apos;].gsub(&#47;&quot;&#47;, &apos;\&quot;&apos;)
            if not product_attr[&apos;barcode&apos;]
                product_attr[&apos;barcode&apos;] = product_attr[&apos;magento_sku&apos;]
            end
            if product_attr[&apos;x_magerp_special_price&apos;] and product_attr[&apos;x_magerp_special_price&apos;] != 0.0
                product_attr[&apos;list_price&apos;] = product_attr[&apos;x_magerp_special_price&apos;]
            end
            if not product_attr[&apos;sale_ok&apos;] or product_attr[&apos;product_type&apos;] == &apos;configurable&apos;
                next
            end
            if product_attr[&apos;barcode&apos;] and not (product_attr[&apos;name&apos;] and verif.index(product_attr[&apos;name&apos;].downcase) or product_attr[&apos;default_code&apos;] and verif.index(product_attr[&apos;default_code&apos;].downcase) or product_attr[&apos;barcode&apos;] and verif.index(product_attr[&apos;barcode&apos;].downcase))
                list += [product_attr]
            else 
                errors &lt;&lt; product_attr[&apos;default_code&apos;]
            end
            verif &lt;&lt; product_attr[&apos;name&apos;].downcase if product_attr[&apos;name&apos;]
            verif &lt;&lt; product_attr[&apos;default_code&apos;].downcase if product_attr[&apos;default_code&apos;]
            verif &lt;&lt; product_attr[&apos;barcode&apos;].downcase if product_attr[&apos;barcode&apos;]
        end
        #puts &quot;list&quot;, list
        puts &quot;errors (in the erp)&quot;, errors
        if list == []
            puts &apos;Error, all modified products with the following ids have an empty barcode\n&apos; * 1
            puts product_ids
        else
            first_line = list[0]
            other_lines = list - [first_line]
            $output &lt;&lt; first_line
            other_lines
        end
    else # solves bug if there is no line
        puts &apos;No products have been modified&apos; * 1
        #$output &lt;&lt; {&quot;price&quot; =&gt; 0.0, &quot;name&quot; =&gt; &quot;Unknown&quot;, &quot;pos_categ_id&quot; =&gt; pos_categ_id, &quot;default_code&quot; =&gt; &quot;false&quot;, &quot;barcode&quot; =&gt; &quot;false&quot;, &quot;id&quot; =&gt; 0, &quot;modified_date&quot; =&gt; Time.new.strftime(&quot;%Y-%m-%d %H:%M:%S&quot;), &quot;categ_id&quot; =&gt; &quot;All products&quot;, &quot;oe_id&quot; =&gt; 4, &quot;standard_price&quot; =&gt; 1.0, &quot;list_price&quot; =&gt; 1.0}
    end
end

